package sampleJFrame;

public class Cat_SSR extends Animal{
	private static final long serialVersionUID = 1L;
	//コンストラクタのオーバーライド
	Cat_SSR() {
		super();
		super.xVelocity = 0;
		super.yVelocity = 0;
	}
	
	//設定
	protected void prepareImageAndScreenAndVoice() {
		super.image = new ImageIcon(getClass().getClassLoader().getResource("./images/jitensya_neko")).getImage();
	}
}
